// Control structures in Rubber Duck
Auto x = 10;
Auto y = 20;

// If statement
if (x < y) {
    Display("x is less than y");
} else {
    Display("x is greater than or equal to y");
}

// While loop
Auto i = 0;
while (i < 5) {
    Display("Count: " + i);
    i = i + 1;
}

// For loop
for (Auto j = 0; j < 3; j = j + 1) {
    Display("For loop iteration: " + j);
}